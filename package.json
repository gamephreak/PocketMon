    "@types/jest": "^25.1.2",
    "@types/node": "^13.9.2",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.19.0",
    "css-loader": "^1.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint": "^6.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
